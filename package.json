{
  "name": "gameswap-backend",
  "version": "1.0.0",
  "description": "gameswap-backend",
  "main": "index.js",
  "scripts": {
    "clean": "yarn clean:ts && yarn clean:docker",
    "clean:ts": "shx rm -rf lib",
    "clean:docker": "shx rm -rf 0x_mesh/db",
    "build": "tsc -p tsconfig.json",
    "db:migrate": "./node_modules/.bin/typeorm migration:run --config ./lib/src/ormconfig.js ",
    "db:revert": "./node_modules/.bin/typeorm migration:revert --config ./lib/src/ormconfig.js ",
    "integration-test": "mocha --require source-map-support/register --require make-promises-safe lib/integration-test/**/*_test.js --timeout 200000 --exit",
    "test": "yarn test:rest && yarn test:rfqt",
    "test:rest": "env-cmd -f ./test/test_env mocha --exclude lib/test/rfqt_test.js --require source-map-support/register --require make-promises-safe lib/test/**/*_test.js --timeout 200000 --exit --bail",
    "test:rfqt": "env-cmd -f ./test/test_env mocha --require source-map-support/register --require make-promises-safe lib/test/rfqt_**.js --timeout 200000 --exit --bail",
    "dev": "nodemon -r dotenv/config src/index.ts | pino-pretty",
    "start": "node -r dotenv/config lib/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shroom-finance/gameswap-backend.git"
  },
  "keywords": [
    "gswap",
    "backend",
    "ndoe",
    "typescript",
    "postgresql"
  ],
  "author": "Jikun Liao <liaojikundever@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shroom-finance/gameswap-backend/issues"
  },
  "homepage": "https://github.com/shroom-finance/gameswap-backend#readme",
  "devDependencies": {
    "@0x/tslint-config": "^4.1.3",
    "@0x/typescript-typings": "^5.1.6",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.0",
    "@types/pino": "^6.3.5",
    "@types/ws": "^7.4.0",
    "env-cmd": "^10.1.0",
    "make-promises-safe": "^5.1.0",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.3.0",
    "shx": "^0.3.3",
    "ts-lint": "^4.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@0x/assert": "^3.0.21",
    "@0x/asset-swapper": "^5.8.0",
    "@0x/mesh-rpc-client": "^9.4.2",
    "@0x/utils": "^6.2.0",
    "@types/uuid": "^8.3.0",
    "@types/uuid-validate": "^0.0.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "delay": "^4.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.20",
    "pg": "^8.5.1",
    "pino": "^6.11.0",
    "prom-client": "^13.1.0",
    "retry-axios": "^2.4.0",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2",
    "uuid-validate": "^0.0.3",
    "ws": "^7.4.2"
  }
}
